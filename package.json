{
  "name": "video-frame-tools",
  "version": "0.0.4",
  "description": "一个用于提取视频帧的工具",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "changeset": "changeset",
    "version-package": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "video",
    "frame",
    "screenshot"
  ],
  "author": "RaineySpace",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaineySpace/video-frame-tools"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.8",
    "react": "^19.2.3",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  }
}
